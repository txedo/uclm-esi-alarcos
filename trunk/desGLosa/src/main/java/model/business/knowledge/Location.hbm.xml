<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model.business.knowledge">
	<class name="Location" table="locations" lazy="false">
		<id name="id" column="id" type="int">
			<generator class="native" />
		</id>
		<many-to-one name="factory" column="factory_id" class="Factory" cascade="none" lazy="false" />
		<many-to-one name="map" column="map_id" class="Map" cascade="none" lazy="false" />
		<property name="xcoord" column="xcoord" type="float" not-null="false" />
		<property name="ycoord" column="ycoord" type="float" not-null="false" />
	</class>
</hibernate-mapping>