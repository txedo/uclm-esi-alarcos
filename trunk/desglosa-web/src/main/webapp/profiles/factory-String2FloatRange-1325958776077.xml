<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<profile model="model.gl.knowledge.GLFactory" entity="es.uclm.inf_cr.alarcos.desglosa_web.model.Factory">
    <name>String2FloatRange</name>
