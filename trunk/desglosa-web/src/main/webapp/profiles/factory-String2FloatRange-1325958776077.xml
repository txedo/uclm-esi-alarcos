    <mappings>
        <mapping>
            <entityAttr name="id" type="int"/>
            <modelAttr name="id" type="int"/>
        </mapping>
        <mapping>
            <entityAttr name="mostRepresentativeMarket_name" type="string"/>
            <modelAttr name="scale" type="float_range"/>
            <rule>
                <low xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string">Defensa</low>
                <high xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string">Defensa</high>
                <value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:float">1.0</value>
            </rule>
            <rule>
                <low xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string">Sanidad</low>
                <high xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string">Sanidad</high>
                <value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:float">1.5</value>
            </rule>
            <rule>
                <low xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string">Arquitectura</low>
                <high xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string">Arquitectura</high>
                <value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:float">2.0</value>
            </rule>
        </mapping>
        <mapping>
            <entityAttr name="mostRepresentativeMarket_color" type="hexcolor"/>
            <modelAttr name="smokestackColor" type="color"/>
        </mapping>
        <mapping>
            <entityAttr name="numberOfDevelopingSubprojects" type="int"/>
            <modelAttr name="smokestackHeight" type="float"/>
        </mapping>
    </mappings>
    <constants>
        <constant name="color" type="color">
            <value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string">ffffff</value>
        </constant>
    </constants>
    <caption/>
</profile>
