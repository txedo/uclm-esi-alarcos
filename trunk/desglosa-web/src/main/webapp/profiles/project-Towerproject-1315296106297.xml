<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<profile model="model.gl.knowledge.GLTower" entity="es.uclm.inf_cr.alarcos.desglosa_web.model.Project">
    <name>Towerproject</name>
    <description>This is a profile which represents projects in a tower model.</description>
    <mappings>
        <mapping>
            <entityAttr name="id" type="int"/>
            <modelAttr name="id" type="int"/>
        </mapping>
        <mapping>
            <entityAttr name="lineasDeCodigo" type="float"/>
            <modelAttr name="width" type="float"/>
            <ratio xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:int">1000000</ratio>
        </mapping>
        <mapping>
            <entityAttr name="comentarios" type="float"/>
            <modelAttr name="depth" type="float"/>
            <ratio xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:int">10000</ratio>
        </mapping>
        <mapping>
            <entityAttr name="fichajeTotal" type="float"/>
            <modelAttr name="height" type="float"/>
        </mapping>
        <mapping>
            <entityAttr name="ratioFichaje" type="float"/>
            <modelAttr name="innerHeight" type="float"/>
        </mapping>
        <mapping>
            <entityAttr name="audited" type="boolean"/>
            <modelAttr name="color" type="color"/>
            <rule>
                <low xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string">true</low>
                <high xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string"></high>
                <value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string">00ff00</value>
            </rule>
            <rule>
                <low xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string">false</low>
                <high xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string"></high>
                <value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string">ff0000</value>
            </rule>
        </mapping>
    </mappings>
    <constants>
        <constant name="scale" type="float">
            <value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:int">1</value>
        </constant>
    </constants>
    <caption>
        <entry color="00ff00" label="The project is audited"/>
        <entry color="ff0000" label="The project is NOT audited"/>
    </caption>
</profile>
