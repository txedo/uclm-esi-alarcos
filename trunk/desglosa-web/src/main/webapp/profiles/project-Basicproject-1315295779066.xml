<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<profile model="model.gl.knowledge.GLAntennaBall" entity="es.uclm.inf_cr.alarcos.desglosa_web.model.Project">
    <name>Basicproject</name>
    <description>This is a basic project profile.</description>
    <mappings>
        <mapping>
            <entityAttr name="id" type="int"/>
            <modelAttr name="id" type="int"/>
        </mapping>
        <mapping>
            <entityAttr name="repairedIncidences" type="int"/>
            <modelAttr name="leftChildBallValue" type="int"/>
        </mapping>
        <mapping>
            <entityAttr name="nonRepairedIncidences" type="int"/>
            <modelAttr name="rightChildBallValue" type="int"/>
        </mapping>
        <mapping>
            <entityAttr name="size" type="int"/>
            <modelAttr name="parentBallRadius" type="float"/>
            <ratio xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:double">15</ratio>
        </mapping>
        <mapping>
            <entityAttr name="audited" type="boolean"/>
            <modelAttr name="progressionMark" type="boolean"/>
        </mapping>
        <mapping>
            <entityAttr name="market_color" type="hexcolor"/>
            <modelAttr name="color" type="color"/>
        </mapping>
        <mapping>
            <entityAttr name="name" type="string"/>
            <modelAttr name="label" type="string"/>
        </mapping>
    </mappings>
    <constants>
        <constant name="scale" type="float">
            <value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:int">1</value>
        </constant>
    </constants>
    <caption>
        <entry color="ff0000" label="Non fixed incidences"/>
        <entry color="00ff00" label="Fixed incidences"/>
    </caption>
</profile>
