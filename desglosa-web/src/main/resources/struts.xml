	<package name="json" extends="json-default">
		<!-- Interceptors -->
	    <interceptors>
            <!-- Interceptor to handle roles and actions -->
            <interceptor name="allUsersInterceptor" class="allUsersInterceptor"/>
			<interceptor-stack name="AllUsersInterceptor">
            	<interceptor-ref name="allUsersInterceptor"/>
            	<interceptor-ref name="defaultStack" />
            </interceptor-stack>            
        </interceptors>
        
		<global-results>
		    <result name="input" type="json">
		        <param name="ignoreHierarchy">false</param>
		        <param name="includeProperties">actionErrors\[\d+\], fieldErrors\..+$, actionMessages\[\d+\]</param>
		    </result>
		    <result name="error" type="json">
		        <param name="ignoreHierarchy">false</param>
		        <param name="includeProperties">actionErrors\[\d+\], fieldErrors\..+$, actionMessages\[\d+\]</param>
		    </result>
		    <result name="Exception" type="json">
		        <param name="ignoreHierarchy">false</param>
		        <param name="includeProperties">actionErrors\[\d+\], fieldErrors\..+$, actionMessages\[\d+\]</param>
		    </result>
		</global-results>
		<global-exception-mappings>
		    <exception-mapping exception="java.lang.Throwable" result="Exception" />
		</global-exception-mappings>
	
		<action name="json_p_*" class="profileAction" method="{1}">
			<interceptor-ref name="AllUsersInterceptor"/>
			<result name="success" type="json">
				<param name="noCache">true</param>
			</result>
		</action>
        <action name="json_m_*" class="marketAction" method="{1}">
        	<interceptor-ref name="AllUsersInterceptor"/>
            <result name="success" type="json">
                <param name="noCache">true</param>
            </result>
        </action>
        <action name="json_factoryById" class="visualizationAction" method="factoryById">
        	<interceptor-ref name="AllUsersInterceptor"/>
            <result name="success" type="json">
                <param name="noCache">true</param>
                <param name="includeProperties">
                    factories\[\d+\]\.id,
                    factories\[\d+\]\.company\.name,
                    factories\[\d+\]\.name,
                    factories\[\d+\]\.information,
                    factories\[\d+\]\.director.*,
                    factories\[\d+\]\.email,
                    factories\[\d+\]\.employees,
                    factories\[\d+\]\.address.*,
                    factories\[\d+\]\.location.*,
                    factories\[\d+\]\.numberOfLeadingProjects,
                    factories\[\d+\]\.numberOfDevelopingSubprojects,
                    factories\[\d+\]\.projects\[\d+\]\.id,
                    factories\[\d+\]\.projects\[\d+\]\.name
                </param>
            </result>
        </action>
        <action name="json_factoriesByCompanyId" class="visualizationAction" method="factoriesByCompanyId">
        	<interceptor-ref name="AllUsersInterceptor"/>
            <result name="success" type="json">
                <param name="noCache">true</param>
                <param name="includeProperties">
                    factories\[\d+\]\.id,
                    factories\[\d+\]\.company\.name,
                    factories\[\d+\]\.name,
                    factories\[\d+\]\.information,
                    factories\[\d+\]\.director.*,
                    factories\[\d+\]\.email,
                    factories\[\d+\]\.employees,
                    factories\[\d+\]\.address.*,
                    factories\[\d+\]\.location.*,
                    factories\[\d+\]\.numberOfLeadingProjects,
                    factories\[\d+\]\.numberOfDevelopingSubprojects,
                    factories\[\d+\]\.projects\[\d+\]\.id,
                    factories\[\d+\]\.projects\[\d+\]\.name
                </param>
            </result>
        </action>
        <action name="json_projectsBy*yId" class="visualizationAction" method="projectsBy{1}yId">
        	<interceptor-ref name="AllUsersInterceptor"/>
            <result name="success" type="json">
                <param name="noCache">true</param>
                <param name="includeProperties">
                    projects\[\d+\]\.id,
                    projects\[\d+\]\.name,
                    projects\[\d+\]\.subprojects\[\d+\]\.id,
                </param>
            </result>
        </action>
        <action name="json_projectById" class="visualizationAction" method="projectById">
        	<interceptor-ref name="AllUsersInterceptor"/>
            <result name="success" type="json">
                <param name="noCache">true</param>
                <param name="includeProperties">
                    projects\[\d+\]\.subprojects\[\d+\]\.id,
                    projects\[\d+\]\.subprojects\[\d+\]\.name,
                    projects\[\d+\]\.subprojects\[\d+\]\.factory\.id,
                    projects\[\d+\]\.subprojects\[\d+\]\.factory\.company\.name,
                    projects\[\d+\]\.subprojects\[\d+\]\.factory\.name,
                    projects\[\d+\]\.subprojects\[\d+\]\.factory\.information,
