<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC 
	"-//SPRING//DTD BEAN//EN" 
	"http://www.springframework.org/dtd/spring-beans.dtd">
	
<beans>
	<!-- Example of SAF2 action instantiated by Spring -->
    <bean id="helloWorldAction" class="es.uclm.inf_cr.alarcos.desglosa_web.actions.HelloWorldAction" singleton="false" />
    
    <!-- SAF2 actions instantiated by Spring -->
    <bean id="indexAction" class="es.uclm.inf_cr.alarcos.desglosa_web.actions.IndexAction" singleton="false" />
    <bean id="startVisualizationAction" class="es.uclm.inf_cr.alarcos.desglosa_web.actions.StartVisualizationAction" singleton="false" />
    <bean id="showAdminPanelAction" class="es.uclm.inf_cr.alarcos.desglosa_web.actions.ShowAdminPanelAction" singleton="false" />
    <bean id="showMyAccountAction" class="es.uclm.inf_cr.alarcos.desglosa_web.actions.ShowMyAccountAction" singleton="false" />
    <bean id="contactUsAction" class="es.uclm.inf_cr.alarcos.desglosa_web.actions.ContactUsAction" singleton="false" />
</beans>
