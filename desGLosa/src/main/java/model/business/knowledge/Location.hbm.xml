<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model.business.knowledge">
	<class name="Location" table="locations">
		<composite-id>
			<key-many-to-one name="map" column="maps_id" class="Map" lazy="false" />
		</composite-id>
		<property name="xcoord" column="xcoord" type="float" not-null="false" />
		<property name="ycoord" column="ycoord" type="float" not-null="false" />
	</class>
</hibernate-mapping>