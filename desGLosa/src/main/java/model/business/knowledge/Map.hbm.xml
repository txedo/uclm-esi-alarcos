<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model.business.knowledge">
	<class name="Map" table="maps" lazy="false">
		<id name="id" column="id" type="int">
			<generator class="native" />
		</id>
		<property name="parentId" column="parentId" type="int" not-null="false" />
		<property name="label" column="label" type="string" not-null="false" />
		<property name="checksum" column="checksum" type="string" not-null="false" />
		<many-to-one name="image" column="images_id" unique="true" not-null="false" class="Image" cascade="all" lazy="false" />
	</class>
</hibernate-mapping>